<launch>

    <!-- Start recorder first -->
    <node name="record_and_convert"
          pkg="franka_interactive_controllers"
          type="record_and_convert.py"
          output="screen" />
  
    <!-- Start controller after -->
    <node name="damm_controller"
          pkg="lpvds_damm"
          type="damm_node.py"
          output="screen">
      <param name="position_model_json" value="$(find lpvds_damm)/trained_ds/j_shape_position.json" />
      <param name="orientation_model_json" value="$(find lpvds_damm)/trained_ds/j_shape_orientation.json" />
    </node>
  
  </launch>
  